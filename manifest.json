{
  "manifest_version": 3,
  "name": "myLichessHomePage",
  "version": "1.3",

  "description": "my Lichess Home Page",
  
  "icons": {
    "48": "icons/mlhp-48.png",
    "96": "icons/mlhp-96.png"
    },
"permissions": [
  "storage"
],
  "content_scripts": [
    {
      "matches": ["*://lichess.org/*"],
      "js": ["dontwanna.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
